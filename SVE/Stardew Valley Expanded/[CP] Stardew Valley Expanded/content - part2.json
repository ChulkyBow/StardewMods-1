    {
      "Action": "EditData",
      "Target": "Characters/Dialogue/MarriageDialogueClaire",
	  "When": { "Date": "Fall 15" }, //Mon
      "Entries": {
      "funReturn_Claire": "Let's get some rest, dear. It's a big day tomorrow.$1#$e#*yawn*...$5",
      "funLeave_Claire": "Are you prepared for the town fair? I know you've been working hard this season.$1#$e#I wonder what baby animals there will be..$5"
        }
	},
    {
      "Action": "EditData",
      "Target": "Characters/Dialogue/MarriageDialogueClaire",
	  "When": { "Date": "Fall 17" }, //Wed
      "Entries": {
      "funReturn_Claire": "I washed the dishes and swept the house when I got back from work.$1#$e#I felt like being produtive today...$5",
      "funLeave_Claire": "Today is going to be great. I can feel it...$1"
